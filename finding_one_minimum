from random import randint

list_of_numbers = [randint(1, 100) for _ in range(10)]

print(list_of_numbers)

min_1 = list_of_numbers[0]

for index in range(len(list_of_numbers) - 1):
    if list_of_numbers[index] < min_1:
        min_1 = list_of_numbers[index]

print(f"Found minimum value: {min_1}")
